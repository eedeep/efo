            margin-top: -1px;
        }
    }

    /* 
        Deprecate #article-feature-image in favour of object
        i.e:
            id="object-feature-image" class="{{ object.content_type }}"
    */
    
    #object-feature-image {
        .row();
        .top();
        clear: both;
        position: relative;

        figure {
            display: block;
            width: 640px; /* this will stretch the img */
            img {
                /* 
                @Stewart: Need to research someway to make the figure width: 640px 
                and not stretch the img at the same time, and center it.
                */
                margin:0px auto;
                width:auto;
            }
            figcaption {
                position: relative;
                display: block;
                margin: 0px;
                .object-feature-image-count {
                    font-weight: bold;
                }
                .object-feature-image-caption {
                    display: table-cell;
                    vertical-align: top;
                    width:550px;
                    background-color:#e5e5e5;
                    color:#000;
                    height: 34px;
                    font-size: 12px;
                    line-height: 16px;
                    padding:9px 84px 10px 12px;
                    .object-credits-photographers,
                    .object-credits-photographers * {
                        color: #4d4d4d;
                    }
                }   
            }
        }   
        .object-slideshow-controls {
            z-index: 99999;
            position: absolute;
            right: 11px;
            top: 12px;
        
            img{
                margin:-2px;
            }
        
            .object-slideshow-next,
            .object-slideshow-previous {
                width:23px;
                height:30px;
                background-repeat: no-repeat;
                float:left;
                cursor: pointer;
                cursor: hand;
            }
        
            .object-slideshow-next {
                background-image: url(/site_media/static/img/arrow_right.png);
            }
        
            .object-slideshow-previous {
                background-image: url(/site_media/static/img/arrow_left.png);
            }
        
            .object-slideshow-next:hover {
                background-image: url(/site_media/static/img/arrow_right_hover.png);
            }
        
            .object-slideshow-previous:hover {
                background-image: url(/site_media/static/img/arrow_left_hover.png);
            }
        }
    }

    #object-feature-image.slideshow {
        display:block;

        #slideshow-images {
            .object-feature-image-container {
                display: table-cell;
                line-height: 0px; /* this fixes the gap between image and caption -- but not sure why? */
                height: 426px;
                width: 640px;
                vertical-align: bottom;
                text-align: center;
                figure {
                    display: block;
                }
                figure.first {

                }
                figure.sibling {
                    display: none;
                }
            }
        }
    }

/* End: Feature Image & Slideshow ------------------------ */


/* Forms
------------------------------------------------------------ */


    /* Form defaults */
    
    label {
        font-weight: bold;
    }
    fieldset {
        border: 1px solid #ccc;
    }
    legend {
        font-weight: bold;
    }
    input[type=text],input[type=password],input.text,input.title,textarea {
        background-color: #fff;
        border: 1px solid #bbb;
    }
    input[type=submit],input[type=reset] {.rounded(3px);
    }
    input[type=text]:focus,input[type=password]:focus,input.text:focus,input.title:focus,textarea:focus {
        border-color: #666;
    }
    select {
        background-color: #fff;
        border-width: 1px;
        border-style: solid;
    }
    .error,.alert,.notice,.success,.info {
        border: 1px solid #ddd;
    }
    .error,.alert {
        background: #fbe3e4;
        color: #8a1f11;
        border-color: #fbc2c4;
    }
    .notice {
        background: #fff6bf;
        color: #514721;
        border-color: #ffd324;
    }
    .success {
        background: #e6efc2;
        color: #black;
        border-color: #c6d880;
    }
    .info {
        background: #d5edf8;
        color: #205791;
        border-color: #92cae4;
    }
    .error a,.alert a {
        color: #8a1f11;
    }
    .notice a {
        color: #514721;
    }
    .success a {
        color: #264409;
    }
    .info a {
        color: #205791;
    }


    /* django-uniForm styling */

        .uniForm {
            fieldset {
                border:none;
                padding:0px
            }
            input,
            textarea,
            #id_first_name,
            #id_last_name,
            #id_email,
