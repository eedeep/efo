                float:left;
            }
    
            #hint_id_subscribe {
                width:408px;
                padding-left:10px;
                color:#1a1a1a;
            }
    
            .ctrlHolder{
                margin-bottom:20px;
            }
            .checkbox {
                .checkboxinput {
                  float: left;
                  height: 20px;
                  width: 20px;   
                }
                p {
                    font-size: 12px;
                }
                label {
                    float: left;
                }
            }
            label { 
                display:block;
                font-size: 14px;
                float:none;
                font-weight: bold;
